{
    "name": "avoutic/web-framework-stripe",
    "description": "Stripe module for web-framework",
    "type": "library",
    "homepage": "https://web-framework.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Avoutic",
            "email": "avoutic@gmail.com"
        }
    ],
    "require": {
        "stripe/stripe-php": "^7.128",
        "avoutic/web-framework": "^8.0 || ^9.0 || ^10.0",
        "guzzlehttp/guzzle": "^7.9"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "WebFramework\\Stripe\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": false
        }
    }
}
